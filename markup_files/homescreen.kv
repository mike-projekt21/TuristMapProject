#:include markup_files/homeMapView.kv
<HomeScreen>:
    name: 'home'
    BoxLayout:
        id: main_box
        orientation: 'vertical'
        HomeMapView:
        MDBottomAppBar:
            MDTopAppBar:
                icon: "compass-outline"
                type: "bottom"
                mode: "center"
                on_action_button: app.change_screen("home","right")
                left_action_items: [["magnify", lambda x: app.change_screen('search',"right")]]
                right_action_items:[["account", lambda x: app.change_screen("profile","left")]]
